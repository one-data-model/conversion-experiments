<LWM2M xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://openmobilealliance.org/tech/profiles/LWM2M.xsd">
    <Object ObjectType="MODefinition">
        <Name>inverter</Name>
        <Description1>This Resource describes functions for the control and monitoring of IEC 61850 based circuit breaker.</Description1>
        <ObjectID>65535</ObjectID>
        <ObjectURN>urn:oma:lwm2m:ext:65535</ObjectURN>
        <LWM2MVersion>1.0</LWM2MVersion>
        <ObjectVersion>1.0</ObjectVersion>
        <MultipleInstances>Multiple</MultipleInstances>
        <Mandatory>Optional</Mandatory>
        <Resources>
            <Item ID="1">
                <Name>status</Name>
                <Operations>R</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>String</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>The inverter status. The status can only be reset out of bounds.</Description>
            </Item>
            <Item ID="2">
                <Name>ratedpower</Name>
                <Operations>R</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>Float</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>The rated power in kW, defined at manufacturing time.</Description>
            </Item>
            <Item ID="3">
                <Name>minvoltmppt</Name>
                <Operations>R</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>Float</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>Minimum voltage for MPPT (Maximum power point tracking) control method (V), defined at manufacturing time.</Description>
            </Item>
            <Item ID="4">
                <Name>maxvoltmppt</Name>
                <Operations>R</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>Float</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>Maximum voltage for MPPT (Maximum power point tracking) control method (V), defined at manufacturing time.</Description>
            </Item>
            <Item ID="5">
                <Name>inputvoltage</Name>
                <Operations>R</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>Float</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>input voltage in Volts.</Description>
            </Item>
            <Item ID="6">
                <Name>inputcurrent</Name>
                <Operations>R</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>Float</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>input current in Amperes.</Description>
            </Item>
            <Item ID="7">
                <Name>outputpower</Name>
                <Operations>R</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>Float</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>output power in kW.</Description>
            </Item>
            <Item ID="8">
                <Name>timestamp</Name>
                <Operations>R</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>String</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>An RFC3339 formatted time indicating when the data was observed (e.g.: 2016-02-15T09:19Z, 1996-12-19T16:39:57-08:00). Resolution in 1/100 second.</Description>
            </Item>
        </Resources>
    </Object>
</LWM2M>
