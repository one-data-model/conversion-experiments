<LWM2M xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://openmobilealliance.org/tech/profiles/LWM2M.xsd">
    <Object ObjectType="MODefinition">
        <Name>media.audio</Name>
        <Description1>This OCF Resource specifies the audio media types that an OCF Device supports.</Description1>
        <ObjectID>65535</ObjectID>
        <ObjectURN>urn:oma:lwm2m:ext:65535</ObjectURN>
        <LWM2MVersion>1.0</LWM2MVersion>
        <ObjectVersion>1.0</ObjectVersion>
        <MultipleInstances>Multiple</MultipleInstances>
        <Mandatory>Optional</Mandatory>
        <Resources>
            <Item ID="1">
                <Name>mediacore</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>unknown (object)</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>The Media Core Properties common on all Media Resource Types,</Description>
            </Item>
            <Item ID="2">
                <Name>artists</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Multiple</MultipleInstances>
                <Mandatory>Optional</Mandatory>
                <Type>String</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>List of artists that recorded the song</Description>
            </Item>
            <Item ID="3">
                <Name>album</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Optional</Mandatory>
                <Type>String</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>Which album the song and picture belong to (if applicable)</Description>
            </Item>
            <Item ID="4">
                <Name>albumartwork</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Multiple</MultipleInstances>
                <Mandatory>Optional</Mandatory>
                <Type>unknown (object)</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>The array of icons that are used as the album artwork.</Description>
            </Item>
            <Item ID="5">
                <Name>tracknumber</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Optional</Mandatory>
                <Type>Integer</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>The track number from the album</Description>
            </Item>
            <Item ID="6">
                <Name>sdp</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Multiple</MultipleInstances>
                <Mandatory>Optional</Mandatory>
                <Type>String</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>Array of strings, a string for each Session Description Protocol syntax.</Description>
            </Item>
            <Item ID="7">
                <Name>duration</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Optional</Mandatory>
                <Type>String</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>Duration is the total length of the media audio with format pattern according to ISO 8601 duration. For example, P0Y0M0DT0H4M27S represents a duration of 4 minutes, and 27 seconds.</Description>
            </Item>
            <Item ID="8">
                <Name>producers</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Multiple</MultipleInstances>
                <Mandatory>Optional</Mandatory>
                <Type>String</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>List of producers that produced the song</Description>
            </Item>
            <Item ID="9">
                <Name>composers</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Multiple</MultipleInstances>
                <Mandatory>Optional</Mandatory>
                <Type>String</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>List of composers that wrote the song</Description>
            </Item>
            <Item ID="10">
                <Name>language</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Optional</Mandatory>
                <Type>String</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>Current language of the audio media content with format pattern according to RFC 5646 language tag</Description>
            </Item>
        </Resources>
    </Object>
</LWM2M>
