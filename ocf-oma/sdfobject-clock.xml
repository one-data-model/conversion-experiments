<LWM2M xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://openmobilealliance.org/tech/profiles/LWM2M.xsd">
    <Object ObjectType="MODefinition">
        <Name>clock</Name>
        <Description1>This Resource describes the properties associated with clock and time. Clock is a time information. The Property "datetime" is using RFC3339 datetime format (e. g: "2007-04-05T14:30Z") (Time+Date+Timezone) The Property "countdown" is the desired total seconds for countdown.</Description1>
        <ObjectID>65535</ObjectID>
        <ObjectURN>urn:oma:lwm2m:ext:65535</ObjectURN>
        <LWM2MVersion>1.0</LWM2MVersion>
        <ObjectVersion>1.0</ObjectVersion>
        <MultipleInstances>Multiple</MultipleInstances>
        <Mandatory>Optional</Mandatory>
        <Resources>
            <Item ID="1">
                <Name>countdown</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Optional</Mandatory>
                <Type>Float</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>The desired total seconds for countdown.</Description>
            </Item>
            <Item ID="2">
                <Name>datetime</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>String</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>Rhe date time using RFC3339 datetime format (e.g: 2007-04-05T14:30:00Z, 2007-04-05T14:30:00+09:00).</Description>
            </Item>
        </Resources>
    </Object>
</LWM2M>
