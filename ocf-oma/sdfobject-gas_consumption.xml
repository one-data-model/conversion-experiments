<LWM2M xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://openmobilealliance.org/tech/profiles/LWM2M.xsd">
    <Object ObjectType="MODefinition">
        <Name>gas.consumption</Name>
        <Description1>This Resource describes Properties associated with the energy associated with the consumption of natural gas The gas value is in kilowatt hours [kWh]. The volume value is in metres cubed [m3]. Provides the cumulative gas energy, the cumulative gas volume and the calorific value and conversion factor used/required to convert from gas volume (m3[TB1]) to gas energy (KWh).</Description1>
        <ObjectID>65535</ObjectID>
        <ObjectURN>urn:oma:lwm2m:ext:65535</ObjectURN>
        <LWM2MVersion>1.0</LWM2MVersion>
        <ObjectVersion>1.0</ObjectVersion>
        <MultipleInstances>Multiple</MultipleInstances>
        <Mandatory>Optional</Mandatory>
        <Resources>
            <Item ID="1">
                <Name>gas</Name>
                <Operations>R</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>Float</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>gas energy consumed in kWh</Description>
            </Item>
            <Item ID="2">
                <Name>precision</Name>
                <Operations>R</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Optional</Mandatory>
                <Type>Float</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>Accuracy granularity of the exposed value</Description>
            </Item>
            <Item ID="3">
                <Name>volume</Name>
                <Operations>R</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Optional</Mandatory>
                <Type>Float</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>gas volume consumed in m3 (metres cubed)</Description>
            </Item>
        </Resources>
    </Object>
</LWM2M>
