<LWM2M xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://openmobilealliance.org/tech/profiles/LWM2M.xsd">
    <Object ObjectType="MODefinition">
        <Name>media.image</Name>
        <Description1>This OCF Resource specifies the image media types that an OCF Device supports.</Description1>
        <ObjectID>65535</ObjectID>
        <ObjectURN>urn:oma:lwm2m:ext:65535</ObjectURN>
        <LWM2MVersion>1.0</LWM2MVersion>
        <ObjectVersion>1.0</ObjectVersion>
        <MultipleInstances>Multiple</MultipleInstances>
        <Mandatory>Optional</Mandatory>
        <Resources>
            <Item ID="1">
                <Name>mediacore</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>unknown (object)</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>The Media Core Properties common on all Media Resource Types,</Description>
            </Item>
            <Item ID="2">
                <Name>artists</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Multiple</MultipleInstances>
                <Mandatory>Optional</Mandatory>
                <Type>String</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>List of artists</Description>
            </Item>
            <Item ID="3">
                <Name>album</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Optional</Mandatory>
                <Type>String</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>Which album the picture belongs to (if applicable).</Description>
            </Item>
            <Item ID="4">
                <Name>albumartwork</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Multiple</MultipleInstances>
                <Mandatory>Optional</Mandatory>
                <Type>unknown (object)</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>The array of icons that are used as the album artwork.</Description>
            </Item>
            <Item ID="5">
                <Name>width</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Optional</Mandatory>
                <Type>Integer</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>The resolution of the image</Description>
            </Item>
            <Item ID="6">
                <Name>height</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Optional</Mandatory>
                <Type>Integer</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>The resolution of the image</Description>
            </Item>
            <Item ID="7">
                <Name>size</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Optional</Mandatory>
                <Type>Float</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>The size of the image in KB - Kilo-Bytes</Description>
            </Item>
            <Item ID="8">
                <Name>resolution</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Optional</Mandatory>
                <Type>Integer</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>The resolution of the image in pixels</Description>
            </Item>
            <Item ID="9">
                <Name>location</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Optional</Mandatory>
                <Type>String</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>Location is the user-friendly string of the geographic location of the image.</Description>
            </Item>
            <Item ID="10">
                <Name>geolocation</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Optional</Mandatory>
                <Type>unknown (object)</Type>
                <RangeEnumeration/>
                <Units/>
                <Description/>
            </Item>
        </Resources>
    </Object>
</LWM2M>
