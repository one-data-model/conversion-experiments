<LWM2M xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://openmobilealliance.org/tech/profiles/LWM2M.xsd">
    <Object ObjectType="MODefinition">
        <Name>pvconnectionterminal</Name>
        <Description1>This Resource describes functions for the control and monitoring of IEC 61850 based PV Array system connection terminal.</Description1>
        <ObjectID>65535</ObjectID>
        <ObjectURN>urn:oma:lwm2m:ext:65535</ObjectURN>
        <LWM2MVersion>1.0</LWM2MVersion>
        <ObjectVersion>1.0</ObjectVersion>
        <MultipleInstances>Multiple</MultipleInstances>
        <Mandatory>Optional</Mandatory>
        <Resources>
            <Item ID="1">
                <Name>ratedarrayvoltage</Name>
                <Operations>R</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>Float</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>Rated voltage of array (Nominal values of maximum power voltage * number of modules) (V)</Description>
            </Item>
            <Item ID="2">
                <Name>ratedarraycurrent</Name>
                <Operations>R</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>Float</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>Rated current of array (Nominal values of maximum power current * number of modules) (A), defined at manufacturing time.</Description>
            </Item>
            <Item ID="3">
                <Name>arrayvoltage</Name>
                <Operations>R</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>Float</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>Output voltage of array in volts (V).</Description>
            </Item>
            <Item ID="4">
                <Name>arraycurrent</Name>
                <Operations>R</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>Float</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>Output current of array in Ampere (A).</Description>
            </Item>
            <Item ID="5">
                <Name>leakagecurrent</Name>
                <Operations>R</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Optional</Mandatory>
                <Type>Float</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>The leakage current in mA.</Description>
            </Item>
            <Item ID="6">
                <Name>insulationresistance</Name>
                <Operations>R</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Optional</Mandatory>
                <Type>Float</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>Insulation resistance of circuit breaker (M Ohm).</Description>
            </Item>
            <Item ID="7">
                <Name>timestamp</Name>
                <Operations>R</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>String</Type>
                <RangeEnumeration/>
                <Units/>
                <Description>An RFC3339 formatted time indicating when the data was observed (e.g.: 2016-02-15T09:19Z, 1996-12-19T16:39:57-08:00). Note that 1/100 time resolution should be used.</Description>
            </Item>
        </Resources>
    </Object>
</LWM2M>
